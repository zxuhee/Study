#nuke.startPerformanceTimers()
#When You want to reset
#nuke.resetPerformanceTimers()
#nuke.stopPerformanceTimers()


###############################################


nuke.menu('Nuke').addCommand('JUHEE/start_green', 'nuke.startPerformanceTimers()')
nuke.menu('Nuke').addCommand('JUHEE/stop_green', 'nuke.stopPerformanceTimers()')
